  <Printrange>
    <Print_layer value="0"/>
  </Printrange>
  <Xwalls value="1"/>
  <!-- 0: No transverse walls in the 2D simulations; 1: Transverse walls included in 2D simulations -->
  <PileUp value="0"/>
